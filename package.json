{
  "name": "helia-docker",
  "version": "0.0.1",
  "description": "Helia in Docker Container",
  "main": "src/index.ts",
  "scripts": {
    "build": "aegir build --bundle false",
    "start": "node dist/src/index.js",
    "start:dev": "npm run build && node dist/src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "healthcheck": "node dist/src/healthcheck.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whizzzkid/helia-docker.git"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "keywords": [
    "helia",
    "docker",
    "p2p",
    "retrievals"
  ],
  "author": "Nishant Arora (me@nishantarora.in)",
  "license": "Apache-2.0 OR MIT",
  "bugs": {
    "url": "https://github.com/whizzzkid/helia-docker/issues"
  },
  "homepage": "https://github.com/whizzzkid/helia-docker#readme",
  "devDependencies": {
    "@types/express": "4.x",
    "@types/mime-types": "2.x",
    "@types/node": "20.x",
    "aegir": "40.x",
    "debug": "4.3.4",
    "typescript": "5.x"
  },
  "dependencies": {
    "@helia/unixfs": "1.x",
    "express": "4.x",
    "express-prom-bundle": "6.x",
    "file-type": "18.x",
    "helia": "2.x",
    "lru-cache": "10.x",
    "mime-types": "2.x",
    "p-try-each": "1.x"
  }
}
